{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss 'GMT'} {EventCode} {Level} {OrleansLoggerType} {Caller} {IPEndPoint} {SourceContext}]: {Message}{NewLine}{Exception}{NewLine}"
        }
      }
    ]
  }
}